
@{
    ViewData["Title"] = "Contact";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <style>
        body {
            font-family: Arial, sans-serif;
            background-color: #f8f9fa;
            color: #333;
            padding: 50px 0;
        }

        .container {
            max-width: 800px;
            margin: 0 auto;
        }

        .contact-info {
            margin-bottom: 20px;
        }

            .contact-info img {
                margin-right: 10px;
            }
    </style>
</head>
<body>
    <div class="container">
        <h1>Contact Us</h1>

        <div class="contact-info">
            <img src="/images/Phone Icon.jpg" alt="Phone Icon" width="30" height="30">
            <span>Phone: +1234567890</span>
        </div>

        <div class="contact-info">
            <img src="/images/Email Icon.jpg" alt="Email Icon" width="30" height="30">
            <span>Email: example@example.com</span>
        </div>

        <div class="contact-info">
            <img src="/images/Home Icon.jpg" alt="Home Icon" width="30" height="30">
            <span>Address: 123 Street Name, City, Country</span>
        </div>
    </div>
</body>
</html>

